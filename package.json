{
  "name": "eth-history",
  "version": "0.1.4",
  "description": "Utility for retrieving historical block/event data for contract address",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require babel-core/register --require babel-polyfill $(find ./src -name *.test.js)",
    "prepare": "babel ./src --out-dir ./dist --ignore '**/*.test.js'",
    "build": "npm run prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buidlhub/eth-history.git"
  },
  "keywords": [
    "ethereum",
    "blockchain",
    "events",
    "tracking"
  ],
  "author": "TekHedz, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buidlhub/eth-history/issues"
  },
  "homepage": "https://github.com/buidlhub/eth-history#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "eth-sync": "^0.1.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "dotenv": "^8.0.0",
    "mocha": "^6.2.0",
    "web3": "^1.2.1"
  }
}
